function onLoad() {

    // content styling
    
    const content = document.getElementById('content');
    content.style.color = 'yellow';

    // child creation and styling

    let central = document.createElement('div');
    content.appendChild(central);
    central.outerHTML =  `<div class="nav"> <h1>delicious-thai</h1> <ul><li class="tabs tab1">about us</li><li class="tabs tab2">menu</li><li class="tabs tab3">contact us</li></ul> </div> <div class="description"> <h1> have you tried thai flavors?</h1> <p>you see, you can see, whatever loreops</p> </div>`;    
    
    
}

export { onLoad };
